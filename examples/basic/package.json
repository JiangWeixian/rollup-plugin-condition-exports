{
  "name": "examples-basic",
  "version": "1.8.0",
  "license": "MIT",
  "homepage": "https://github.com/spring-catponents/rollup-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spring-catponents/rollup-template.git"
  },
  "bugs": {
    "url": "https://github.com/spring-catponents/rollup-template/issues",
    "email": "jiangweixian1994@gmail.com"
  },
  "author": {
    "name": "JW",
    "email": "jiangweixian1994@gmail.com",
    "url": "https://twitter.com/jiangweixian"
  },
  "scripts": {
    "build": "rimraf cjs es && DEBUG=rpce* rollup -c --silent",
    "build:array": "rimraf cjs es && DEBUG=rpce* rollup -c ./rollup.config.array.js --silent",
    "build:pattern": "rimraf cjs es && DEBUG=rpce* rollup -c ./rollup.config.pattern.js --silent",
    "build:alias": "rimraf cjs es && DEBUG=rpce* rollup -c ./rollup.config.alias.js --silent",
    "build:components": "rimraf cjs es && DEBUG=rpce* rollup -c ./rollup.config.components.js --silent",
    "build:next": "rimraf cjs es && DEBUG=rpce* rollup -c ./rollup.config.next.js --silent"
  },
  "dependencies": {
    "functional-md": "^1.1.0",
    "rollup-plugin-condition-exports": "workspace:^1.4.1",
    "rollup-plugin-size": "^0.2.2"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.4",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/jest": "27.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.58.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.20",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-typescript2": "0.30.0",
    "ts-node": "10.2.1",
    "tslib": "2.3.1",
    "typescript": "4.4.3"
  },
  "exports": {
    "./about": {
      "require": "./dist/about/index.cjs",
      "import": "./dist/about/index.mjs",
      "types": "./dist/about/index.d.ts"
    },
    "./components": {
      "require": "./dist/components.cjs",
      "import": "./dist/components.mjs",
      "types": "./dist/components.d.ts"
    },
    "./disabled-condition": {
      "require": "./dist/disabled-condition.cjs",
      "import": "./dist/disabled-condition.mjs",
      "types": "./dist/disabled-condition.d.ts"
    },
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./blog/*": {
      "require": "./dist/blog/[...all].cjs",
      "import": "./dist/blog/[...all].mjs",
      "types": "./dist/blog/[...all].d.ts"
    },
    "./blog/:id": {
      "require": "./dist/blog/[id].cjs",
      "import": "./dist/blog/[id].mjs",
      "types": "./dist/blog/[id].d.ts"
    },
    "./blog": {
      "require": "./dist/blog/index.cjs",
      "import": "./dist/blog/index.mjs",
      "types": "./dist/blog/index.d.ts"
    },
    "./condition/browser": {
      "require": "./dist/condition/browser.cjs",
      "import": "./dist/condition/browser.mjs",
      "types": "./dist/condition/browser.d.ts"
    },
    "./condition/node": {
      "require": "./dist/condition/node.cjs",
      "import": "./dist/condition/node.mjs",
      "types": "./dist/condition/node.d.ts"
    },
    "./disabled-condition/node": {
      "require": "./dist/disabled-condition/node.cjs",
      "import": "./dist/disabled-condition/node.mjs",
      "types": "./dist/disabled-condition/node.d.ts"
    },
    "./blog/today/*": {
      "require": "./dist/blog/today/[...all].cjs",
      "import": "./dist/blog/today/[...all].mjs",
      "types": "./dist/blog/today/[...all].d.ts"
    },
    "./blog/today": {
      "require": "./dist/blog/today/index.cjs",
      "import": "./dist/blog/today/index.mjs",
      "types": "./dist/blog/today/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "about": [
        "dist/about/index.d.ts"
      ],
      "components": [
        "dist/components.d.ts"
      ],
      "disabled-condition": [
        "dist/disabled-condition.d.ts"
      ],
      "blog/*": [
        "dist/blog/[...all].d.ts"
      ],
      "blog/:id": [
        "dist/blog/[id].d.ts"
      ],
      "blog": [
        "dist/blog/index.d.ts"
      ],
      "condition/browser": [
        "dist/condition/browser.d.ts"
      ],
      "condition/node": [
        "dist/condition/node.d.ts"
      ],
      "disabled-condition/node": [
        "dist/disabled-condition/node.d.ts"
      ],
      "blog/today/*": [
        "dist/blog/today/[...all].d.ts"
      ],
      "blog/today": [
        "dist/blog/today/index.d.ts"
      ]
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts"
}
