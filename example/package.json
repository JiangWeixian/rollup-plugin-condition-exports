  "license": "MIT",
  "homepage": "https://github.com/spring-catponents/rollup-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spring-catponents/rollup-template.git"
  },
  "bugs": {
    "url": "https://github.com/spring-catponents/rollup-template/issues",
    "email": "jiangweixian1994@gmail.com"
  },
  "author": {
    "name": "JW",
    "email": "jiangweixian1994@gmail.com",
    "url": "https://twitter.com/jiangweixian"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./md": {
      "require": "./dist/md.cjs",
      "import": "./dist/md.mjs",
      "types": "./dist/md.d.ts"
    },
    "./feature": {
      "browser": {
        "require": "./dist/feature/browser.cjs",
        "import": "./dist/feature/browser.mjs",
        "types": "./dist/feature/browser.d.ts"
      },
      "node": {
        "require": "./dist/feature/node.cjs",
        "import": "./dist/feature/node.mjs",
        "types": "./dist/feature/node.d.ts"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf cjs es && DEBUG=rpce* rollup -c --silent"
  },
  "dependencies": {
    "functional-md": "^1.1.0",
    "rollup-plugin-condition-exports": "workspace:^1.4.1",
    "rollup-plugin-size": "^0.2.2"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.4",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/jest": "27.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.58.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.20",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-typescript2": "0.30.0",
    "ts-node": "10.2.1",
    "tslib": "2.3.1",
    "typescript": "4.4.3"
  },
  "typesVersions": {
    "*": {
      "md": [
        "dist/md.d.ts"
      ],
      "feature": [
        "dist/feature/node.d.ts"
      ]
    }
  }
}
