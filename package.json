{
  "name": "rollup-plugin-condition-exports",
  "version": "2.0.0-next.1",
  "license": "MIT",
  "homepage": "https://github.com/JiangWeixian/rollup-plugin-condition-exports#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JiangWeixian/rollup-plugin-condition-exports.git"
  },
  "bugs": {
    "url": "https://github.com/JiangWeixian/rollup-plugin-condition-exports/issues",
    "email": "jiangweixian1994@gmail.com"
  },
  "author": {
    "name": "JW",
    "email": "jiangweixian1994@gmail.com",
    "url": "https://twitter.com/jiangweixian"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    }
  },
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rimraf lib && rollup -c --silent",
    "dev": "rollup -c -w --silent",
    "test": "vitest",
    "pretest": "pnpm run build",
    "ci:version": "pnpm changeset version",
    "ci:publish": "pnpm run build && pnpm changeset publish",
    "ci:snapshot": "pnpm changeset version --snapshot beta",
    "ci:prerelease": "pnpm run build && pnpm changeset publish --tag beta",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/**/*.{js,ts,tsx,vue,json}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@antfu/utils": "^0.7.0",
    "@rollup/pluginutils": "^4.1.2",
    "debug": "^4.3.2",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.0"
  },
  "devDependencies": {
    "@aiou/eslint-config": "^0.6.1",
    "@changesets/cli": "^2.17.0",
    "@rollup/plugin-alias": "^3.1.4",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.9",
    "@types/set-value": "^4.0.1",
    "cz-emoji": "^1.3.1",
    "esbuild": "^0.15.15",
    "eslint": "^8.28.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.1",
    "lodash-es": "^4.17.21",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "rollup": "2.60.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-node-externals": "^5.0.2",
    "tslib": "2.3.1",
    "typescript": "4.4.3",
    "vitest": "^0.25.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-emoji"
    }
  }
}
